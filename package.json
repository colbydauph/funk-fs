{
  "name": "funk-fs",
  "version": "0.3.0",
  "engines": {
    "node": ">= 8.2"
  },
  "description": "Functional File System Utilities for Node.js",
  "author": "Colby Dauphinais <colby@dauphina.is> (https://colby.dauphina.is)",
  "contributors": [
    "Colby Dauphinais <colby@dauphina.is> (https://colby.dauphina.is)"
  ],
  "dependencies": {
    "ramda": "~0.25.0"
  },
  "devDependencies": {
    "chai": "~4.1.2",
    "chai-as-promised": "~7.1.1",
    "eslint": "~4.12.0",
    "eslint-config-defaults": "colbydauph/eslint-config-defaults#1.0.0",
    "memfs": "~2.6.0",
    "mocha": "~4.0.1",
    "nyc": "~11.3.0",
    "sinon": "~4.1.2"
  },
  "main": "index.js",
  "scripts": {
    "cover": "NODE_ENV=test nyc --check-coverage npm test -- --reporter=progress || { echo \"\\033[0;32mview coverage\\033[0m\" `pwd`/coverage/index.html; exit 1; }",
    "init": "rm -rf ./node_modules; npm install;",
    "lint": "eslint --max-warnings=10 $(find . ! -path '*node_modules*' ! -path '*dist*' ! -path '*coverage*' -type f | egrep '[.]js?$') && echo '\\033[0;32mLinting - Passed\\033[0m'",
    "start": "node .",
    "test-watch": "mocha --reporter=min --watch $(find . ! -path '*node_modules*' -name '*.test.js')",
    "test": "NODE_ENV=test mocha --reporter=list $(find . ! -path '*node_modules*' -name '*.test.js')",
    "verify": "npm run init && npm run lint && npm run cover"
  },
  "nyc": {
    "lines": 60,
    "statements": 60,
    "functions": 30,
    "branches": 75,
    "reporter": [
      "html",
      "text-summary"
    ],
    "watermarks": {
      "statements": [
        90,
        99
      ],
      "branches": [
        90,
        99
      ],
      "functions": [
        90,
        99
      ],
      "lines": [
        90,
        99
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/colbydauph/dauphinais-server.git"
  },
  "bugs": {
    "url": "https://github.com/colbydauph/dauphinais-server/issues"
  },
  "private": true,
  "license": "UNLICENSED"
}
